{
  "dependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "cspell": "^8.10.4",
    "eslint": "^9.7.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3"
  },
  "scripts": {
    "spell-check": "cspell \"**/*.{ts}\"",
    "typecheck": "tsc --noEmit",
    "lint": "prettier -c '/*.{ts?(x),?(c)js?(x),json,y?(a)ml,md}' && eslint --ignore-path .gitignore '/*.ts'",
    "prepare": "husky",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
